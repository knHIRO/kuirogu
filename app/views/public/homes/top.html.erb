<div class="container">
  <div class="row">
    <div class="col-md-12">
    <% @postings.each do |posting| %>
      <% if posting.profile_image.attached? %>
        <%= image_tag posting.profile_image, size: "200x200" %>
      <% else %>
        <%= image_tag 'no_image', size: "200x200" %>
      <% end %>
      <p><%= posting.body %></p>
      <% end %>

    </div>
  </div>
</div>
